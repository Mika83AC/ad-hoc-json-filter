{
   "compilerOptions": {
      "module": "commonjs",
      "preserveConstEnums": true,
      "declaration": true,
      "allowUnreachableCode": false,
      "alwaysStrict": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "noUncheckedIndexedAccess": true,
      "strictNullChecks": true
   }
}